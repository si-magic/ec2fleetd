{
	"$schema": "http://schema.snart.me/fleet-init/json/userdata/0",

	"timeout": 600, // 10 minutes

	"domains": {
		"A.test.ec2fleetd": {
			"attach-volume": [
				{
					"device": "/dev/xvdf",
					"source": "x",
					"volume-id": "vol-NNNNNNNNNNNNNNNNN",
					"exec": [
						{
							"lines": [
								{
									"argv": [
										"../../dump-1.sh",
										"cache/A.test.ec2fleetd@b-dev-xvdf",
										"{all_json}"
									]
								}
							]
						}
					]
				}
			]
		},

		"B.test.ec2fleetd": {
			"attach-volume": [
				{
					"critical": false,
					"device": "/dev/xvdg",
					"source": "pc",
					"pool-name": "b-1",
					"create": {
						"Size": 1, // should fail due to invalid size
						"VolumeType": "st1",
						"TagSpecifications": [
							{
								"ResourceType": "volume",
								"Tags": [
									{
										"Key": "Name",
										"Value": "B.test.ec2fleetd@b-1"
									}
								]
							}
						]
					},
					"exec": [
						{
							"lines": [
								{
									"argv": [
										"../../dump-1.sh",
										"cache/B.test.ec2fleetd@b-dev-xvdg",
										"{all_json}"
									]
								}
							]
						}
					]
				},
				{
					"device": "/dev/xvdh",
					"source": "pc",
					"pool-name": "b-2",
					"create": {
						"Size": 10,
						"VolumeType": "gp3",
						"TagSpecifications": [
							{
								"ResourceType": "volume",
								"Tags": [
									{
										"Key": "Name",
										"Value": "B.test.ec2fleetd@b-2"
									}
								]
							}
						]
					},
					"exec": [
						{
							"lines": [
								{
									"argv": [
										"../../dump-1.sh",
										"cache/B.test.ec2fleetd@b-dev-xvdh",
										"{all_json}"
									]
								}
							]
						}
					]
				},
				{
					"device": "/dev/xvdi",
					"source": "pc",
					"pool-name": "b-3",
					"create": {
						"Size": 10,
						"SnapshotId": "oh snap!", // should cause error
						"VolumeType": "gp3",
						"TagSpecifications": [
							{
								"ResourceType": "volume",
								"Tags": [
									{
										"Key": "Name",
										"Value": "B.test.ec2fleetd@b-3"
									}
								]
							}
						]
					},
					"exec": [
						{
							"lines": [
								{
									"argv": [
										"../../dump-1.sh",
										"cache/B.test.ec2fleetd@b-dev-xvdi",
										"{all_json}"
									]
								}
							]
						}
					]
				}
			]
		}
	}
}

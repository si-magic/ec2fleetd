{
	"$schema": "http://schema.si-magic.com/fleet-init/json/userdata/0",

	"timeout": 600, // 10 minutes

	"set-hostname": "ec2fleetd",

	"domains": {
		"A.test.ec2fleetd": {
			"notify": [
				{
					"backend": "ans-sqs",
					"options": {
						"queue-url": "QUEUE_URL"
					},
					"matrix": {
						"fail": { "enabled": true },
						"start": { "enabled": true },
						"stop": { "enabled": true },
						"interrupt": { "enabled": true }
					}
				},
				{
					"backend": "aws-sns",
					"options": {
						"region": "REGION",
						"topic": "TOPIC_ARN"
					},
					// "envelope": {
					// 	"subject": "SUBJECT",
					// 	"body": "BODY"
					// },
					"matrix": {
						"fail": { "enabled": true },
						"start": { "enabled": true },
						"stop": { "enabled": true },
						"interrupt": { "enabled": true }
					}
				}
			]
		}
	}
